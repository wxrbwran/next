{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    ["import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": false
      }, "ant"],
    ["@babel/plugin-proposal-decorators", {
      "decoratorsBeforeExport": false
    }],
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }]
  ]
}
